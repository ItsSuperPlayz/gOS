@echo off
chcp 65001 >nul
title gOS Recoded build 0
setlocal EnableDelayedExpansion
"%~dp0\cursorsize" 0
for /f %%a in ('copy /z "%~f0" nul') do set "CR=%%a"
for /f %%A in ('"prompt $H&for %%B in (1) do rem"') do set "BS=%%A"
for /F %%a in ('echo prompt $E ^| cmd') do set "ESC=%%a"
mode con:cols=80 lines=26
call :color
call :logo
call :reset
for /f "tokens=4 delims=. " %%a in ('ver ^| find /i "version"') do if %%a neq 10 (
echo %red%Failed: gOS is not compatible with your version of Windows.
echo %red%Please use Windows 10 or above to continue using gOS.
pause >nul
exit
)
timeout 1 /nobreak >nul
:draw
call :drawcalc
:ctrl
ver >nul
"%~dp0\getkey"
for %%a in (-75;97;-77;100;-72;119;-80;115;13;102) do if "%errorlevel%"=="%%a" (goto ctrl%errorlevel%)
goto ctrl
:ctrl-75
:ctrl97 left
if %tab%==1 goto ctrl
set /a tab-=1
set select=1
goto draw
:ctrl-77
:ctrl100 right
if %tab%==3 goto ctrl
set /a tab+=1
set select=1
goto draw
:ctrl-72
:ctrl119 up
if %tab%==3 goto ctrl
if %select%==1 goto ctrl
set /a select-=1
goto draw
:ctrl-80
:ctrl115 down
if %tab%==3 goto ctrl
if %tab%==1 (
if %select%==4 goto ctrl
)
if %tab%==2 (
if %select%==5 goto ctrl
)
set /a select+=1
goto draw
:ctrl13
:ctrl102
if %tab%==1 goto main
if %tab%==2 goto exit
goto ctrl
:main
if %select%==1 goto winact
if %select%==2 goto activator
if %select%==3 (
call :info_ps
start /i /wait powershell -command "irm https://christitus.com/win | iex" 2>nul
call :info_psdone
)
if %select%==4 (
call :info_ps
start /i /wait powershell -command "iwr -useb https://git.io/debloat|iex" 2>nul
call :info_psdone
)
goto ctrl
:exit
if %select%==1 exit
if %select%==2 shutdown /s /t 0
if %select%==3 shutdown /r /t 0
ver >nul
if %select%==4 shutdown /s /fw /t 0 2>nul
if %errorlevel% neq 0 (<nul set /p =!ESC![9;5H%redbg%This feature is not supported on your hardware.%reset%)
if %select%==5 shutdown /r /o /t 0
goto ctrl
:winact
::DELETE LINE BELOW IF FINISHED
goto ctrl
call :act_confirm
choice /c yn >nul 2>nul
if %errorlevel%==2 goto draw
call :content
call :act_prg2
:activator
call :act_confirm
choice /c yn >nul 2>nul
if %errorlevel%==2 goto draw
call :content
call :act_prg
<nul set /p =!ESC![16;17H.!BS!%reset%         Checking internet connection...       
ver >nul
ping google.com >nul
if %errorlevel% neq 0 (
<nul set /p =!ESC![16;17H.!BS!%red%         Failed: No internet connection.       
<nul set /p =!ESC![17;17H.!BS!%reset%            Press any key to close.
pause >nul
goto draw
)
<nul set /p =!ESC![16;17H.!BS!%reset%           Detecting Office version...         
if exist "%ProgramFiles%\Microsoft Office\Office14\ospp.vbs" (
cd /d "%ProgramFiles%\Microsoft Office\Office14"
<nul set /p =!ESC![11;47H%green%2010
goto office14
)
if exist "%ProgramFiles(x86)%\Microsoft Office\Office14\ospp.vbs" (
cd /d "%ProgramFiles(x86)%\Microsoft Office\Office14"
<nul set /p =!ESC![11;47H%green%2010
goto office14
)
if exist "%ProgramFiles%\Microsoft Office\Office15\ospp.vbs" (
cd /d "%ProgramFiles%\Microsoft Office\Office15"
<nul set /p =!ESC![11;47H%green%2013
goto office15
)
if exist "%ProgramFiles(x86)%\Microsoft Office\Office15\ospp.vbs" (
cd /d "%ProgramFiles(x86)%\Microsoft Office\Office15"
<nul set /p =!ESC![11;47H%green%2013
goto office15
)
if exist "%ProgramFiles%\Microsoft Office\Office16" (
cd /d "%ProgramFiles%\Microsoft Office\Office16"
goto office16
)
if exist "%ProgramFiles(x86)%\Microsoft Office\Office16" (
cd /d "%ProgramFiles(x86)%\Microsoft Office\Office16"
goto office16
)
<nul set /p =!ESC![11;47H%red%FAILED
<nul set /p =!ESC![16;17H.!BS!%red% Failed: No Office was found on your computer.
<nul set /p =!ESC![17;17H.!BS!%reset%            Press any key to close.
pause >nul
goto draw
:office14
<nul set /p =!ESC![16;17H.!BS!%reset%          Installing activation key...         
cscript ospp.vbs /inpkey:VYBBJ-TRJPB-QFQRF-QFT4D-H3GVB >%temp%\log.txt
cscript ospp.vbs /inpkey:YC7DK-G2NP3-2QQC3-J6H88-GVGXT >>%temp%\log.txt
<nul set /p =!ESC![12;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%           Changing activation host...         
cscript ospp.vbs /sethst:kms8.msguides.com >>%temp%\log.txt
cscript ospp.vbs /setprt:1688 >>%temp%\log.txt
<nul set /p =!ESC![13;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%                 Activating...                 
cscript ospp.vbs /act >>%temp%\log.txt
goto activator2
:office15
<nul set /p =!ESC![11;47H%green%2013
goto office14
:office16
for /f %%x in ('dir /b ..\root\Licenses16\ProPlus2021VL_KMS*.xrm-ms') do cscript ospp.vbs /inslic:"..\root\Licenses16\%%x" >nul && goto 2021
for /f %%x in ('dir /b ..\root\Licenses16\ProPlus2019VL*.xrm-ms') do cscript ospp.vbs /inslic:"..\root\Licenses16\%%x" >nul && goto 2019
for /f %%x in ('dir /b ..\root\Licenses16\proplusvl_kms*.xrm-ms') do cscript ospp.vbs /inslic:"..\root\Licenses16\%%x" >nul && goto 2016
exit
:2016
<nul set /p =!ESC![11;47H%green%2016
<nul set /p =!ESC![16;17H.!BS!%reset%          Installing activation key...         
cscript ospp.vbs /inpkey:XQNVK-8JYDB-WJ9W3-YJ8YR-WFG99 >%temp%\log.txt
cscript ospp.vbs /unpkey:BTDRB >>%temp%\log.txt
cscript ospp.vbs /unpkey:KHGM9 >>%temp%\log.txt
cscript ospp.vbs /unpkey:CPQVG >>%temp%\log.txt
<nul set /p =!ESC![12;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%           Changing activation host...         
cscript ospp.vbs /sethst:107.175.77.7 >>%temp%\log.txt
cscript ospp.vbs /setprt:1688 >>%temp%\log.txt
