cscript ospp.vbs /unpkey:6MWKP >%temp%\log.txt
cscript ospp.vbs /inpkey:NMMKJ-6RK4F-KMJVX-8D9MJ-6MWKP >>%temp%\log.txt
<nul set /p =!ESC![12;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%           Changing activation host...         
cscript ospp.vbs /sethst:107.175.77.7 >>%temp%\log.txt
cscript ospp.vbs /setprt:1688 >>%temp%\log.txt
<nul set /p =!ESC![13;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%                 Activating...                 
cscript ospp.vbs /act >>%temp%\log.txt
goto activator2
:2021
<nul set /p =!ESC![11;47H%green%2021/365
<nul set /p =!ESC![16;17H.!BS!%reset%          Installing activation key...         
cscript ospp.vbs /unpkey:6F7TH >%temp%\log.txt
cscript ospp.vbs /inpkey:FXYTK-NJJ8C-GB6DW-3DYQT-6F7TH >>%temp%\log.txt
<nul set /p =!ESC![12;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%           Changing activation host...         
cscript ospp.vbs /sethst:107.175.77.7 >>%temp%\log.txt
cscript ospp.vbs /setprt:1688 >>%temp%\log.txt
<nul set /p =!ESC![13;47H%green%DONE
<nul set /p =!ESC![16;17H.!BS!%reset%                 Activating...                 
cscript ospp.vbs /act >>%temp%\log.txt
:activator2
ver >nul
find /i "Product activation successful" %temp%\log.txt >nul
if %errorlevel%==1 (
<nul set /p =!ESC![14;47H%red%NOT ACTIVATED
<nul set /p =!ESC![16;17H.!BS!%red%   Failed: Something happened in the process.  
<nul set /p =!ESC![17;17H.!BS!%reset%            Press any key to close.
pause >nul
) else (
<nul set /p =!ESC![14;47H.!BS!             
<nul set /p =!ESC![14;47H%green%ACTIVATED
<nul set /p =!ESC![16;17H.!BS!%green%             Activation successful.            
<nul set /p =!ESC![17;17H.!BS!%reset%            Press any key to close.
pause >nul
)
del %temp%\log.txt
goto draw
:logo
echo. & echo. & echo. & echo. & echo. & echo. & echo. & echo. & echo.
echo                                      _____ _____
echo                                _____/     \     \
echo                               /     ^|  ^|  ^|  ^|__/
echo                               ^|  ^|  ^|  ^|  ^|__   \
echo                               ^|  ^|  ^|  ^|  ^|  ^|  ^|
echo                               \__   \_____/_____/
echo                               \_____/
echo. & echo.
goto :eof
:color
set red=!ESC![31m
set cyan=!ESC![36m
set green=!ESC![32m
set redbg=!ESC![41m
set invert=!ESC![7m
set reset=!ESC![0m
goto :eof
:drawcalc
<nul set /p =!ESC![0;0H
echo                                        gOS
set erase=                                                                           │
set "t1=%reset%" & set "t2=%reset%" & set "t3=%reset%"
set "line1=%erase%" & set "line2=%erase%" & set "line3=%erase%" & set "line4=%erase%" & set "line5=%erase%" & set "line6=%erase%"
set "s1=%reset%" & set "s2=%reset%" & set "s3=%reset%" & set "s4=%reset%" & set "s5=%reset%"
set t%tab%=%invert%
set s%select%=%invert%

if %tab%==1 (
set line1=%s1%Windows 10/11 activator ^(WIP^)%reset%                                              │
set line2=%s2%Office activator%reset%                                                           │
set line3=%s3%[PowerShell] Chris Titus's Windows Utility%reset%                                 │
set line4=%s4%[PowerShell] Windows10Debloater%reset%                                            │
)
if %tab%==2 (
set line1=%s1%Exit gOS%reset%                                                                   │
set line3=%s2%Shutdown computer%reset%                                                          │
set line4=%s3%Restart computer%reset%                                                           │
set line5=%s4%Restart + enter BIOS%reset%                                                       │
set line6=%s5%Enter Advanced Boot Options menu%reset%                                           │
)
if %tab%==3 (
set line1=gOS Recoded build 0 - Feb 13th 2024                                        │
)
echo   %t1%  MAIN  %reset%  %t2%  EXIT  %reset%  %t3%  ABOUT  %reset%
::80x22 box
