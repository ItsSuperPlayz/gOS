@echo off
cls
echo.
title gOS Recoded Launcher
echo        _____ _____
echo  _____/     \     \
echo /     ^|  ^|  ^|  ^|__/
echo ^|  ^|  ^|  ^|  ^|__   \
echo ^|  ^|  ^|  ^|  ^|  ^|  ^|
echo \__   \_____/_____/
echo \_____/ build 0
echo. & echo.
echo WARNING: USE THIS TOOL AT YOUR OWN RISK!
echo NO RESPONSIBILITIES ARE TAKEN FOR ANY DAMAGE CAUSED
echo BY YOUR OWN ACTIONS.
echo THIS TOOL DOES NOT DAMAGE YOUR COMPUTER ON ITS OWN!
echo. & echo.
echo Downloading files
cd /d "%temp%"
set folder=%random%%random%%random%%random%%random%
mkdir %folder%
cd %folder%
curl --silent https://raw.githubusercontent.com/ItsSuperPlayz/gOS/main/gos >gos.bat
curl --silent https://raw.githubusercontent.com/ItsSuperPlayz/gOS/main/htb >htb.bat
curl --silent https://raw.githubusercontent.com/ItsSuperPlayz/gOS/main/hchar >hchar.vbs
curl --silent https://raw.githubusercontent.com/ItsSuperPlayz/gOS/main/cursorsize >cursorsize.exe.hex
curl --silent https://raw.githubusercontent.com/ItsSuperPlayz/gOS/main/getkey >getkey.exe.hex
echo Unpacking files
cmd /c htb cursorsize.exe.hex >nul
cmd /c htb getkey.exe.hex >nul
echo Finishing up
del cursorsize.exe.hex
del getkey.exe.hex
del htb.bat
del hchar.vbs
echo Starting gOS
echo. & echo.
echo PLEASE KEEP THIS WINDOW OPEN WHILE gOS IS RUNNING.
echo n|start /wait gos.bat 2>nul
echo. & echo.
cd ..
rmdir /s /q %folder%
echo Thank you for using gOS.
echo.
echo Press any key to return to Command Prompt.
pause >nul
